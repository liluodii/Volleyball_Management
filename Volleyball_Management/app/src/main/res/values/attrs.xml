<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DTextView">
        <attr name="textFontFace" format="enum">
            <enum name="REGULAR" value="1" />
            <enum name="SEMIBOLD" value="2" />
            <enum name="MEDIUM" value="3" />
            <enum name="BOLD" value="4" />
        </attr>
        <attr name="leftDrawable" format="reference" />
        <attr name="rightDrawable" format="reference" />
        <attr name="topDrawable" format="reference" />
        <attr name="bottomDrawable" format="reference" />
        <attr name="tintColor" format="color" />
    </declare-styleable>

    <declare-styleable name="SimpleRatingBar">
        <attr name="srb_borderColor" format="color" />
        <attr name="srb_fillColor" format="color" />
        <attr name="srb_backgroundColor" format="color" />
        <attr name="srb_starBackgroundColor" format="color" />
        <attr name="srb_pressedBorderColor" format="color" />
        <attr name="srb_pressedFillColor" format="color" />
        <attr name="srb_pressedBackgroundColor" format="color" />
        <attr name="srb_pressedStarBackgroundColor" format="color" />
        <attr name="srb_numberOfStars" format="integer" />
        <attr name="srb_starSize" format="dimension" />
        <attr name="srb_maxStarSize" format="dimension" />
        <attr name="srb_starsSeparation" format="dimension" />
        <attr name="srb_starBorderWidth" format="float" />
        <attr name="srb_starCornerRadius" format="float" />
        <attr name="srb_isIndicator" format="boolean" />
        <attr name="srb_rating" format="float" />
        <attr name="srb_stepSize" format="float" />
        <attr name="srb_drawBorderEnabled" format="boolean" />
        <attr name="srb_gravity" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="GoogleMapsBottomSheetBehavior_Layout">
        <attr name="behavior_peekHeight"/>
        <attr name="behavior_hideable" format="boolean" />
        <attr name="behavior_skipCollapsed" format="boolean" />
        <attr name="behavior_anchorHeight" format="dimension">
            <enum name="auto" value="-1"/>
        </attr>
        <attr name="behavior_anchorColor" format="color" />
        <attr name="behavior_collapsedColor" format="color" />
        <attr name="behavior_anchorTextColor" format="color" />
        <attr name="behavior_collapsedTextColor" format="color" />
        <attr name="behavior_header_layout" format="reference" />
        <attr name="behavior_content_layout" format="reference" />
        <attr name="behavior_parallax_layout" format="reference" />
    </declare-styleable>

</resources>